{
    // "editor.tabSize": 2,
    // "editor.indentSize": "tabSize",
    "[cpp]": {
        "editor.tabSize": 2,
        "editor.indentSize": "tabSize",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "third_party": true,
        "build": true,
        "bits": true,
        "**/Cargo.lock": true,
        "atcoder": true,
        "inner": true,
        "tests": true,
        "LICENSE": true,
        "consts.h": true,
        "consts.h.in": true,
        "testlib.h": true,
        "README.md": true,
        "**/target": true,
        "Template": true,
        // "tools": true,
        "minify": true,
        "compare": true,
        ".clang-format": true,
        "**/.git*": true,
        "Cargo.toml": true,
        "install.sh": true,
    },
    "cmake.environment": {
        "GTEST_BRIEF": "1",
        "ENABLE_THIRD_PARTY": "true",
        // "ENABLE_TESTING": "true",
        "ENABLE_BINS": "true",
        "ENABLE_ARCHIVE": "true",
    },
    "rust-analyzer.cargo.extraEnv": {
        "RUST_LOG": "info",
    }
}
