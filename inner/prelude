#define VI std::vector<int>
#define ll long long
#define ULL unsigned ll
#define ALL(x) x.begin(), x.end()
#define llu unsigned long long
#define il inline
#define fr friend

namespace inner {

#define TKASE      \
  int T;           \
  scanf("%d", &T); \
  for (int kase = 1; kase <= T; ++kase)
  template<const int mod = 998244353>
  ll qp(ll a, ll n);
  ll qp(ll a, ll n, int mod);
  namespace IO {
    template<class T>
    struct ns {
      T t;
    };
    template<class T>
    ns<T> NoSuffix(T t) {
      return ns<T>{.t = t};
    }
    template<class T, class... U>
    void R(T &head, U &...tail);
    template<class T, class... U>
    void W(const ns<T> &head, const U &...tail);
    template<class T, class... U>
    void W(const T &head, const U &...tail);
  }// namespace IO
}// namespace inner

namespace inner {
  namespace IO {
    template<class T>
    void _R(T &x) { std::cin >> x; }
    void _R(int &x) { scanf("%d", &x); }
    void _R(ll &x) { scanf("%lld", &x); }
    void _R(double &x) { scanf("%lf", &x); }
    void _R(char &x) { x = getchar(); }
    void _R(char *x) { scanf("%s", x); }
    template<class T, class U>
    void _R(std::pair<T, U> &x) { _R(x.first), _R(x.second); }
    template<class T>
    void _R(std::vector<T> &x) {
      for (auto &i: x) _R(i);
    }
    void R() {}
    template<class T, class... U>
    void R(T &head, U &...tail) { _R(head), R(tail...); }
    template<class T>
    void _W(const T &x) { std::cout << x; }
    template<class T>
    void _W(const ns<T> &x) { std::cout << x.t; }
    void _W(const int &x) { printf("%d", x); }
    void _W(const ll &x) { printf("%lld", x); }
    void _W(const double &x) { printf("%.16f", x); }
    void _W(const char &x) { putchar(x); }
    void _W(const char *x) { printf("%s", x); }
    template<class T, class U>
    void _W(const std::pair<T, U> &x) { _W(x.first), putchar(' '), _W(x.second); }
    template<class T>
    void _W(const std::set<T> &x) {
      for (auto i = x.begin(); i != x.end(); _W(*i++))
        if (i != x.cbegin()) putchar(' ');
    }
    template<class T>
    void _W(const std::vector<T> &x) {
      for (auto i = x.begin(); i != x.end(); _W(*i++))
        if (i != x.cbegin()) putchar(' ');
    }
    void W() {}
    template<class T, class... U>
    void W(const ns<T> &head, const U &...tail) { _W(head), W(tail...); }
    template<class T, class... U>
    void W(const T &head, const U &...tail) { _W(head), putchar(sizeof...(tail) ? ' ' : '\n'), W(tail...); }
  }// namespace IO

  template<const int mod>
  ll qp(ll a, ll n) {
    ll res = 1;
    n %= mod - 1;
    if (n < 0) n += mod - 1;
    while (n > 0) {
      if (n & 1) res = res * a % mod;
      a = a * a % mod;
      n >>= 1;
    }
    return res;
  }

  constexpr ll safe_mod(ll x, ll m) {
    x %= m;
    if (x < 0) x += m;
    return x;
  }

  ll qp(ll x, ll n, unsigned int m) {
    if (m == 1) return 0;
    ULL r = 1;
    ULL y = safe_mod(x, m);
    while (n) {
      if (n & 1) r = (r * y) % m;
      y = (y * y) % m;
      n >>= 1;
    }
    return r;
  }
}// namespace inner


